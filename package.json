{
  "name": "spring-streaming",
  "version": "1.0.0",
  "description": "SPA boilerplate on Spring Boot with WebSockets and React",
  "private": true,
  "main": "src/main/www/main.js",
  "author": "Maksim Kostromin",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dageerok/spring-streaming.git"
  },
  "scripts": {
    "postinstall": "bower install",
    "build": "npm i && webpack --config npm/webpack/prod.js -p ",
    "start": "npm i && webpack --config npm/webpack/dev.js -w",
    "test": " npm i && webpack --config npm/webpack/dev.js"
  },
  "dependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.6",
    "bower": "^1.7.7",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "jquery": "^2.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-router-active-component": "^4.0.0-rc.0",
    "stompjs": "^2.3.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "websocket": "^1.0.22"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "react-html-attrs",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "config": {
    "bower_directory": "src/main/www/sockjs"
  },
  "bugs": {
    "url": "https://github.com/dageerok/spring-streaming/issues"
  },
  "homepage": "https://github.com/dageerok/spring-streaming#readme",
  "keywords": [
    "webpack",
    "babel",
    "es6",
    "jsx",
    "react",
    "react-router",
    "react-router-active-component",
    "bootstrap",
    "jquery"
  ]
}
